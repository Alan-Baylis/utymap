canvas|z15 {
    grid-cells: 10;
    max-area: 0.0005;
    color-noise-freq: 0.1;
    ele-noise-freq: 0.05;
    color: gradient(#dcdcdc 0%, #c0c0c0 10%, #a9a9a9 50%, #808080);
}

area|z15[amenity],
area|z15[natural],
area|z15[leisure],
area|z15[historic=memorial],
area|z15[landuse] {
    clip:true;
    builders: terrain;
    max-area: 0.0005;
    color-noise-freq: 0.1;
    ele-noise-freq: 0.05;
    color: gradient(#10ff00, #0ff000 50%, #098a00);
}

area|z15[landuse=residential] {
    color: gradient(#a9a9a9, #a9a9a9 30%, #707070 70%, #556b2f);
    texture: ground;
}
area|z15[landuse=cemetery],
area|z15[landuse=grave_yard],
area|z15[amenity=grave_yard] {
    color: gradient(#98fb98, #064807 10%, #035804 50%, #808080);
    texture: ground;
}
area|z15[landuse=farm],
area|z15[landuse=farmland] {
    color: gradient(#15ffcd, #0ead8b 50%, #064739);
    texture: ground;
}
area|z15[landuse=commercial] {
    color: gradient(#999999, #606060);
    texture: ground;
}
area|z15[landuse=retail] {
    color: gradient(#162bff, #0f1dad 50%, #060c47);
    texture: ground;
}
area|z15[landuse=brownfield],
area|z15[landuse=landfill],
area|z15[landuse=greenfield] {
    color: gradient(#fcdd76, #f4a460 20%, #406126);
    texture: grass;
}
area|z15[landuse=construction] {
    color: gradient(#fcdd76, #f4a460 40%, #707070);
    texture: ground;
}
area|z15[amenity=parking][parking=surface] {
    color: gradient(#c0c0c0, #999999 40%, #707070);
    texture: asphalt;
}
area|z15[natural=beach] {
    color: gradient(#fcdd76, #f4a460);
    texture: sand;
}
area|z15[amenity=university],
area|z15[amenity=college],
area|z15[amenity=school],
area|z15[amenity=hospital],
area|z15[amenity=kindergarten] {
    color: gradient(#fcdd76, #a9a9a9 20%, #909090 70%, #556b2f);
    texture: ground;
}

area|z15[landuse=industrial],
area|z15[landuse=railway] {
    color: gradient(#7d7d7d, #707070);
    texture: ground;
}
area|z15[leisure=park] {
    color: gradient(#98fb98, #adff2f 5%, #006400 10%, #556b2f 80%, #b8860b);
    texture: grass;
}
area|z15[natural=wood],
area|z15[landuse=forest] {
    forest: true;
    color: gradient(#98fb98, #064807 10%, #032804 50%, #b8860b);
    foliage-color: gradient(#80c34c, #406126 50%, #101809);
    foliage-texture: grass;
    trunk-color: gradient(#f4a460, #614126 50%, #302013);
    trunk-texture: bark;
    tree-freq: 0.05;
    texture: grass;
}
area|z15[landuse=farmland],
area|z15[landuse=scrub] {
    color: gradient(#adff2f, #006400 30%, #556b2f 90%, #b8860b);
    texture: grass;
}
area|z15[landuse=village_green],
area|z15[landuse=grass],
area|z15[landuse=meadow] {
    color: gradient(#adff2f, #006400 30%, #556b2f 90%, #b8860b);
    texture: grass;
}
area|z15[leisure=recreation_ground] {
    color: gradient(#0d81ff, #0b6fdb 50%, #063b75);
    texture: grass;
}
